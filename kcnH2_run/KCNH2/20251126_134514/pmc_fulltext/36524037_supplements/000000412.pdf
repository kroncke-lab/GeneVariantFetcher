  <script type="text/javascript">

    // 医療関係者以外のジャンプページ
    var NOT_MEDICAL_PERSON_URL = "https://patients.eisai.jp/";
    // CATログインURL
    var CAT_LOGIN_URL = "https://medical.cat.eisai.jp/login?cat_refurl=";
    // CAT新規登録URL
    var CAT_REGIST_URL = "https://medical.cat.eisai.jp/registration?cat_refurl=";
    // cookie指定ドメイン
    const MORDAL_COOKIE_DOMAIN = "eisai.jp";

    MODAL_COOKIE = {
      init: function () {
        var request_uri = decodeURIComponent($.cookie('request_base_uri'));
        var request_url = location.protocol + '//' + location.host + request_uri + location.search + location.hash;
        var encoded_request_url = encodeURIComponent(request_url);

        // i-Searchクローラー対応
        var ua = navigator.userAgent.toLocaleLowerCase();  // userAgent
        var check_ua = 'i-search-crawler';                 // チェック対象UA（i-SearchのUAは「i-search-crawler / 2.0」）

        // チェック対象のUA（i-Searchクローラー）の場合はcookieを書き込み処理を終了する（遷移させる）
        if (ua.indexOf(check_ua) !== -1) {
          $.cookie('modalWindow', 'close', { path: "/", domain: MORDAL_COOKIE_DOMAIN });
          location.href = request_url;
          return false;
        }

        //医療関係者
        $(document).on('click', '.hcp-user', function (e) {
          $.cookie('modalWindow', 'close', { path: "/", domain: MORDAL_COOKIE_DOMAIN });
          const p = e.target.closest('a');
          pageTracker($(p).attr('id'), 2, $(p).attr('value'), request_url, request_url);
        });

        //医療関係者以外
        $(document).on('click', '.other-user', function (e) {
          const p2 = e.target.closest('a');
          pageTracker($(p2).attr('id'), 2, $(p2).attr('value'), request_url, NOT_MEDICAL_PERSON_URL);
        });

        //medパスでログイン 新規会員登録
        $(document).on('click', '#occupation5', function (e) {
          var url = CAT_LOGIN_URL + encoded_request_url;
          pageTracker($(e.target).attr('id'), 2, $(e.target).attr('value'), request_url, url);
        });

        //新規会員登録
        $(document).on('click', '#occupation6', function (e) {
          var url = CAT_REGIST_URL + encoded_request_url;
          pageTracker($(e.target).attr('id'), 2, $(e.target).attr('value'), request_url, url);
        });

      }
    };

    function pageTracker(linkId, target, occupation, requestUrl, locationHref) {
      window.dataLayer = window.dataLayer || [];
      dataLayer.push({
        event: 'medical_confirm',
        occupation: occupation,
        from_url: location.href
      });
      if (target == 1) {
        return true;
      } else {
        var timeout = 200;
        setTimeout("setHref('" + locationHref + "')", timeout);
        return false;
      }
    }
    function setHref(locationHref) {
      location.href = locationHref;
    }

    $(function () {
      MODAL_COOKIE.init();
    });
  </script>
  <!-- Wrap -->
  <div id="wrap">
    <div id="inline_occupation" class="occupation-popup">
      <div class="occupation-popup__inner">
        <!-- Header -->
        <div class="header__logo">
          <a href="//7242392.hs-sites.com?hsLang=ja"><img
