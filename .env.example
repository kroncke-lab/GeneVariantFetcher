# GeneVariantFetcher Environment Variables
# Copy to .env and replace values with real secrets. Do not commit .env.

# Primary LLM provider (required)
OPENAI_API_KEY=your-openai-api-key-here
# Alternate env var name supported by automated_workflow.py
AI_INTEGRATIONS_OPENAI_API_KEY=your-openai-api-key-here

# NCBI (required for PubMed API)
NCBI_EMAIL=your_email@example.com

# Optional but recommended
NCBI_API_KEY=your-ncbi-api-key-here

# Optional provider keys (only needed if used)
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
ELSEVIER_API_KEY=your-elsevier-api-key-here
WILEY_API_KEY=your-wiley-api-key-here
