<tr>
<td valign="top" align="left" rowspan="1" colspan="1">c.7617+1G &gt; T</td>
<td valign="top" align="center" rowspan="1" colspan="1">Intronic</td>
<td valign="top" align="center" rowspan="1" colspan="1">r.7436_7617del</td>
<td valign="top" align="left" rowspan="1" colspan="1">Asp2479Alafs<sup>∗</sup>8</td>
<td valign="top" align="center" rowspan="1" colspan="1">Pathogenic</td>
<td valign="top" align="left" rowspan="1" colspan="1">PVS1, PS3, PM2, PP3: Pathogenic</td>
<td valign="top" align="center" rowspan="1" colspan="1">Class-4</td>
</tr>
<tr>
<td valign="top" align="left" rowspan="1" colspan="1">c.7617+2T &gt; G</td>
<td valign="top" align="center" rowspan="1" colspan="1">Intronic</td>
<td valign="top" align="center" rowspan="1" colspan="1">r.7436_7617del</td>
<td valign="top" align="left" rowspan="1" colspan="1">Asp2479Alafs<sup>∗</sup>8</td>
<td valign="top" align="center" rowspan="1" colspan="1">Pathogenic</td>
<td valign="top" align="left" rowspan="1" colspan="1">PVS1, PS3, PM2, PP3: Pathogenic</td>
<td valign="top" align="center" rowspan="1" colspan="1">Class-4</td>
</tr>
